{
  "name": "undetermini",
  "version": "0.0.9",
  "description": "Library to be able to test LLM base UseCase",
  "author": "Sraleik",
  "license": "MIT",
  "scripts": {
    "build": "tsc && vite build",
    "build:docs": "typedoc && cp -r ./image ./docs/image",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test:unit": "vitest --config ./vitest.config.ts --single-thread"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sraleik/undetermini.git"
  },
  "bugs": {
    "url": "https://github.com/sraleik/undetermini/issues"
  },
  "keywords": [
    "llm",
    "testing"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "./dist/undetermini.js",
  "type": "module",
  "module": "./dist/undetermini.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/undetermini.js",
      "require": "./dist/undetermini.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "console-table-printer": "^2.11.2",
    "currency.js": "^2.0.4",
    "js-tiktoken": "^1.0.7",
    "lokijs": "^1.5.12"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@knodes/typedoc-plugin-pages": "^0.23.4",
    "@types/jest": "^29.5.2",
    "@types/lokijs": "^1.5.11",
    "@types/node": "^20.3.1",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "cohere-ai": "^6.2.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "langchain": "^0.0.166",
    "nanoid": "^4.0.2",
    "prettier": "^3.0.0",
    "testcontainers": "^9.9.1",
    "tinybench": "^2.5.0",
    "tsx": "^3.12.7",
    "typedoc": "0.23.28",
    "typescript": "^5.1.3",
    "uuid": "^9.0.0",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.32.2",
    "vitest-mock-extended": "^1.1.3",
    "zod": "^3.22.4"
  }
}
